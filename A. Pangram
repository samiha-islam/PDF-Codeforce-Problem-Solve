//https://codeforces.com/problemset/problem/520/A
#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    set<char>st;
    string s;
    cin >> s;

    for(int i=0;i<n;i++) {
        st.insert(tolower(s[i]));
    }
    if(st.size() == 26)
        cout << "YES";
    else
        cout << "NO";

    return 0;
}
