//https://codeforces.com/problemset/problem/44/A
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
   set<pair<string,string>>unique;
    for(int i=0;i<n;i++)
    {
        string species,color;
        cin>>species>>color;
        unique.insert({species,color});
    }
    cout<<unique.size()<<endl;
    return 0;
}
