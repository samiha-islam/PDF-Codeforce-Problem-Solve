//https://codeforces.com/problemset/problem/22/A
#include<bits/stdc++.h>
using namespace std;
int main()
{
int n;
cin>>n;
set<int>s;
for(int i=0;i<n;i++)
{
    int x;
    cin>>x;
    s.insert(x);

}
if(s.size()<2)
{
    cout<<"No"<<endl;
}
else
{
    vector<int>v(s.begin(),s.end());
    for(int i=0;i<v.size();i++)
    {
        cout<<v[1]<<endl;
        break;
    }
}
return 0;
}
