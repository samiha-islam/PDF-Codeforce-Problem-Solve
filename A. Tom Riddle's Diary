//https://codeforces.com/contest/855/problem/A
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    set<string>s;
    for(int i=0;i<n;i++)
    {
        string name;
        cin>>name;
        if(s.count(name))
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
            s.insert(name);
        }
    }
    return 0;
}
