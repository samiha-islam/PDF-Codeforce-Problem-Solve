#include <bits/stdc++.h>
using namespace std;

int firstOccurrence(int arr[],int n,int x) {
    int l=0, r=n-1;
    int ans=-1;
    while (l<=r) {
        int m=l+(r-l)/2;
        if (arr[m]==x) {
            ans=m;
            r= m-1;
        } else if(arr[m]<x) {
            l= m+1;
        } else {
            r=m-1;
        }
    }
    return ans;
}
int lastOccurrence(int arr[],int n,int x) {
    int l=0,r =n-1;
    int ans=-1;
    while (l<=r) {
        int m=l+(r-l)/2;
        if (arr[m]==x) {
            ans=m;
            l=m+1;
        } else if (arr[m]<x) {
            l=m+1;
        } else {
            r=m-1;
        }
    }
    return ans;
}

int main() {
    int n;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++) {
        cin>>arr[i];
    }
    int x;
    cin>>x;

    int first =firstOccurrence(arr, n, x);
    int last =lastOccurrence(arr, n, x);

    cout<<first<< " " <<last<< endl;
}
