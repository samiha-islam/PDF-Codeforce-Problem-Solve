#include<bits/stdc++.h>
using namespace std;
int binarysearch(int arr[],int n,int x)
{
    int l=0,r=n-1;
    while(l<=r)
    {
        int m=l+(r-l)/2;
        if(arr[m]==x) return m;
        else if(arr[m]<x) l=m+1;
        else r=m-1;
    }
    return -1;
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int x;
    cin>>x;
    int index=binarysearch(arr,n,x);
    cout<<index<<endl;
}
