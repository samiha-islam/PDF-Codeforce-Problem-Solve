#include <bits/stdc++.h>
using namespace std;

int lowerBound(int arr[],int n,int x) {
    int l=0, r=n-1;
    int ans=n; 
    while (l <= r) {
        int m=l+(r-l)/2;
        if (arr[m]>=x) {
            ans=m;   
            r=m-1;  
        } else {
            l=m+1;  
        }
    }
    return ans;
}

int main() {
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++) cin >> arr[i];
    int x;
    cin>>x;

    cout<<lowerBound(arr,n,x)<<endl;
}
