//https://codeforces.com/problemset/problem/4/C
#include <bits/stdc++.h>
using namespace std;
int main() {
    map<string,int>m;
    int n;
    cin >> n;
    while(n--) {
        string name;
        cin>>name;
        m[name]++;
        if(m[name]==1)
        {
          cout<<"OK\n";
        }
        else {
            cout<<name<< m[name]-1<<endl;
        }
    }
    return 0;
}
