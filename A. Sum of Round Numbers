//https://codeforces.com/problemset/problem/1352/A
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n;
        cin >> n;
        vector<int> v;
        int place=1; 
        while (n > 0) {
            int digit = n % 10;
            if (digit != 0) {
                v.push_back(digit * place);
            }
            n /= 10;
            place *= 10;
        }
        cout << v.size() << "\n";
        for (int i=v.size()-1;i>=0;i--) { 
            cout<<v[i]<<" ";
        }
        cout<<"\n";
    }

    return 0;
}
